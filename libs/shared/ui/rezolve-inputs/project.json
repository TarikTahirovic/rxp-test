{
  "projectType": "library",
  "root": "libs/shared/ui/rezolve-inputs",
  "sourceRoot": "libs/shared/ui/rezolve-inputs/src",
  "prefix": "rezolve-input",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/shared/ui/rezolve-inputs"],
      "options": {
        "project": "libs/shared/ui/rezolve-inputs/ng-package.json",
        "tailwindConfig": "libs/shared/ui/rezolve-inputs/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/ui/rezolve-inputs/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/ui/rezolve-inputs/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/ui/rezolve-inputs"],
      "options": {
        "jestConfig": "libs/shared/ui/rezolve-inputs/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/shared/ui/rezolve-inputs/src/**/*.ts", "libs/shared/ui/rezolve-inputs/src/**/*.html"]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/shared/ui/rezolve-inputs/.storybook"
        },
        "projectBuildConfig": "shared-ui-rezolve-inputs:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/shared-ui-rezolve-inputs",
        "config": {
          "configFolder": "libs/shared/ui/rezolve-inputs/.storybook"
        },
        "projectBuildConfig": "shared-ui-rezolve-inputs:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
